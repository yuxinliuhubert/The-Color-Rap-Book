ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/ColorSlider" "${PODS_CONFIGURATION_BUILD_DIR}/CropViewController" "${PODS_CONFIGURATION_BUILD_DIR}/GhostTypewriter" "${PODS_CONFIGURATION_BUILD_DIR}/LBTAComponents" "${PODS_CONFIGURATION_BUILD_DIR}/LayoutHelper" "${PODS_CONFIGURATION_BUILD_DIR}/RevealingSplashView" "${PODS_CONFIGURATION_BUILD_DIR}/StepSlider" "${PODS_CONFIGURATION_BUILD_DIR}/VegaScrollFlowLayout"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/ColorSlider/ColorSlider.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/CropViewController/CropViewController.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/GhostTypewriter/GhostTypewriter.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/LBTAComponents/LBTAComponents.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/LayoutHelper/LayoutHelper.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/RevealingSplashView/RevealingSplashView.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/StepSlider/StepSlider.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/VegaScrollFlowLayout/VegaScrollFlowLayout.framework/Headers"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_LDFLAGS = $(inherited) -framework "ColorSlider" -framework "CoreGraphics" -framework "CropViewController" -framework "Foundation" -framework "GhostTypewriter" -framework "LBTAComponents" -framework "LayoutHelper" -framework "QuartzCore" -framework "RevealingSplashView" -framework "StepSlider" -framework "UIKit" -framework "VegaScrollFlowLayout"
OTHER_SWIFT_FLAGS = $(inherited) -D COCOAPODS
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
